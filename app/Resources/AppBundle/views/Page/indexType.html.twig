{% extends "admin.html.twig" %}

{% block header %}<h1><i class="fa fa-cube"></i>{{ 'Typy stron'|trans }}</h1>{% endblock %}

{% block content %}
    
    <div class="content">


        {% if objects %}
            <div class="table-wrapper"><table>
                    <tr>
                        <th>{{ 'Nr'|trans }}</th>
                        <th>{{ 'Nazwa'|trans }}</th>
                    </tr>
                {% for object in objects %}
                    <tr>
                        <td>{{object.id}}</td>
                        <td><a href="{{ path('page_type_form', {'id': object.id})}}">{{object.name}}</a></td>
                    </tr>
                {% endfor %}
            </table></div>
        {% else %}
            <p>{{ 'Nie ma typ√≥w stron.'|trans }}</p>
        {% endif %}
    </div>

{% endblock %}