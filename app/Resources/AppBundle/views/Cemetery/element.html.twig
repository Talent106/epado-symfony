<li class="element" data-id="{{ object.id }}"><a class="element" href="#">{{ object.name }}, {{ object.address.city }}<br></a>{% if app.user==object.creator or app.user.type in ['admin','worker','trader'] %}<a href="{{ path('cemetery_form',{'id':object.id}) }}" class="edit"><i class="fa fa-pencil"></i></a>{% endif %}<textarea class="hidden object" >{{object.serialize|json_encode}}</textarea></li>           
